" Filetype detection
filetype on
" Set tab to 2 spaces
set tabstop=2
" Enable line numbers
set number
" Enable syntax highlighting
syntax on
" Fix backspace key...
set backspace=indent,eol,start
" set color on line 81
set colorcolumn=80
highlight ColorColumn ctermbg=0 guibg=lightgrey
" set window title
set title
